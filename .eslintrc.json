{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "radix": "off",
        "array-callback-return": "off",
        "consistent-return": "off",
        "no-param-reassign": "off",
        "import/no-extraneous-dependencies": [
            "off",
            {
                "devDependencies": false,
                "optionalDependencies": false,
                "peerDependencies": false
            }
        ],
        "no-unused-vars": "off"
    }
}